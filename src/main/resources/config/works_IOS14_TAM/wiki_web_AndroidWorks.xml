<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Web Fey Suite" verbose="0" parallel="tests"
	   thread-count="100">
	<listeners>
		<listener class-name="com.quantum.listeners.QuantumReportiumListener" />
	</listeners>

	<test name="fey smoke Web Wiki Android Test" enabled="true"
		  thread-count="3">

		<parameter name="perfecto.capabilities.model" value="Galaxy.*"></parameter>
		<parameter name="perfecto.additional.capabilities" value="{'perfecto:openDeviceTimeout':15}" />
		<parameter name="perfecto.capabilities.automationName" value="Appium"/>
		<groups>
			<run>
				<include name="@first" />
			</run>
		</groups>
		<classes>
			<class
					name="com.qmetry.qaf.automation.step.client.gherkin.GherkinScenarioFactory" />
		</classes>
	</test>
</suite>
